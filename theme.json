{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "border": {},
    "color": {
      "palette": [
        {
          "name": "Primary",
          "slug": "primary",
          "color": "#FF557C"
        },
        {
          "name": "Secondary",
          "slug": "secondary",
          "color": "#1839F2"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#00072F"
        },
        {
          "name": "Grey 200",
          "slug": "grey-200",
          "color": "#CCCDD5"
        },
        {
          "name": "Grey 300",
          "slug": "grey-300",
          "color": "#7F8398"
        },
        {
          "name": "Primary Dark",
          "slug": "primary-dark",
          "color": "#00072F"
        },
        {
          "name": "Light Blue",
          "slug": "light-blue",
          "color": "#1839F207"
        },
        {
          "name": "Light Pink",
          "slug": "light-pink",
          "color": "#FFEEF2"
        },
        {
          "name": "Light Purple",
          "slug": "light-purple",
          "color": "#EFF1FE"
        }
      ]
    },
    "custom": {},
    "dimensions": {},
    "position": {},
    "shadow": {
      "defaultPresets": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fluid": false,
      "fontSizes": [
        {
          "name": "Small",
          "slug": "small",
          "size": "12px"
        },
        {
          "name": "Medium",
          "slug": "medium",
          "size": "18px"
        },
        {
          "name": "Large",
          "slug": "large",
          "size": "20px"
        },
        {
          "name": "Extra Large",
          "slug": "x-large",
          "size": "35px"
        },
        {
          "name": "Extra extra Large",
          "slug": "xx-large",
          "size": "60px"
        }
      ]
    },
    "useRootPaddingAwareAlignments": false,
    "blocks": {
      "core/columns": {
        "spacing": {
          "blockGap": true
        }
      },
      "core/gallery": {
        "spacing": {
          "blockGap": true
        }
      },
      "core/image": {
        "spacing": {
          "blockGap": true
        }
      }
    }
  },
  "styles": {
    "blocks": {
      "core/separator": {
        "color": { "background": " var(--wp--preset--color--grey-200)" },
        "border": { "width": "1px" }
      },
      "core/social-link": {
        "border": { "radius": "0" }
      }
    }
  }
}
